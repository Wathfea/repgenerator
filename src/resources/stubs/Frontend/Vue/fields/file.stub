<div>
    <label class="block text-sm font-medium text-gray-700" for="image">File</label>
    <div class="mt-1">
         <span v-for="file in {{modelNameSingularLowerCase}}.files">
            <template v-if="file.field == '{{field}}'">
               <a :href="file.url">{{file.name}}</a>
            </template>
        </span>
    </div>
</div>
